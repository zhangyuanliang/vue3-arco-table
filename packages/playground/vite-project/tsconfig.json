{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "preserve",
    "strict": true,
    "target": "esnext",
    "module": "esnext",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "lib": ["esnext", "dom"],
    "paths": {
      "@/*": ["src/*"],
    },
    "types": ["vue3-arco-table"]
  },
  "include": ["src/**/*", "src/**/*.vue", ],
  "exclude": ["node_modules"],
  "trailingComma": "all",
}